{
    "swagger": "2.0",
    "info": {
        "description": "jkagent management server.",
        "title": "agent-server",
        "contact": {
            "url": "https://www.jkstack.com"
        },
        "version": "TODO"
    },
    "basePath": "/api",
    "paths": {
        "/agents": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "agents"
                ],
                "summary": "获取节点列表",
                "operationId": "/api/agents",
                "parameters": [
                    {
                        "enum": [
                            "example-agent",
                            "container-agent",
                            "metrics-agent",
                            "..."
                        ],
                        "type": "string",
                        "description": "节点类型,不指定则列出所有类型",
                        "name": "type",
                        "in": "query"
                    },
                    {
                        "minimum": 1,
                        "type": "integer",
                        "default": 1,
                        "description": "分页编号",
                        "name": "page",
                        "in": "query"
                    },
                    {
                        "minimum": 10,
                        "type": "integer",
                        "default": 20,
                        "description": "每页数量",
                        "name": "size",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/api.Success"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "payload": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/definitions/agents.info"
                                            }
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        },
        "/agents/{id}": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "agents"
                ],
                "summary": "获取某个节点信息",
                "operationId": "/api/agents/info",
                "parameters": [
                    {
                        "type": "string",
                        "description": "节点ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/api.Success"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "payload": {
                                            "$ref": "#/definitions/agents.info"
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        },
        "/foo/{id}": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "foo"
                ],
                "summary": "调用example类型的agent",
                "operationId": "/api/foo",
                "parameters": [
                    {
                        "type": "string",
                        "description": "节点ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/api.Success"
                        }
                    }
                }
            }
        },
        "/info/server": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "info"
                ],
                "summary": "获取当前服务器状态",
                "operationId": "/api/info/server",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/api.Success"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "payload": {
                                            "$ref": "#/definitions/info.serverInfo"
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        },
        "/metrics/{id}/dynamic": {
            "get": {
                "description": "当指定top参数时将会获取CPU占用率最高的n个进程数据",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "metrics"
                ],
                "summary": "获取节点的所有动态数据",
                "operationId": "/api/metrics/dynamic",
                "parameters": [
                    {
                        "type": "string",
                        "description": "节点ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "description": "获取进程列表时的数量限制",
                        "name": "top",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/api.Success"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "payload": {
                                            "$ref": "#/definitions/metrics.dynamicInfo"
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        },
        "/metrics/{id}/dynamic/connections": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "metrics"
                ],
                "summary": "获取节点的连接列表数据",
                "operationId": "/api/metrics/dynamic/connections",
                "parameters": [
                    {
                        "type": "string",
                        "description": "节点ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/api.Success"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "payload": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/definitions/metrics.connection"
                                            }
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        },
        "/metrics/{id}/dynamic/process": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "metrics"
                ],
                "summary": "获取节点的所有进程列表数据",
                "operationId": "/api/metrics/dynamic/process",
                "parameters": [
                    {
                        "type": "string",
                        "description": "节点ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "description": "数量限制",
                        "name": "top",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/api.Success"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "payload": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/definitions/metrics.process"
                                            }
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        },
        "/metrics/{id}/dynamic/usage": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "metrics"
                ],
                "summary": "获取节点的usage动态数据",
                "operationId": "/api/metrics/dynamic/usage",
                "parameters": [
                    {
                        "type": "string",
                        "description": "节点ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/api.Success"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "payload": {
                                            "$ref": "#/definitions/metrics.usage"
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        },
        "/metrics/{id}/static": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "metrics"
                ],
                "summary": "获取节点的静态数据",
                "operationId": "/api/metrics/static",
                "parameters": [
                    {
                        "type": "string",
                        "description": "节点ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/api.Success"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "payload": {
                                            "$ref": "#/definitions/metrics.staticInfo"
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "agents.info": {
            "type": "object",
            "properties": {
                "arch": {
                    "description": "操作系统位数",
                    "type": "string",
                    "enum": [
                        "i386",
                        "x86_64",
                        "..."
                    ],
                    "example": "x86_64"
                },
                "id": {
                    "description": "agent-id",
                    "type": "string",
                    "example": "ct-01"
                },
                "ip": {
                    "description": "ip地址",
                    "type": "string",
                    "example": "127.0.0.1"
                },
                "mac": {
                    "description": "mac地址",
                    "type": "string",
                    "example": "00:15:5d:c9:e0:17"
                },
                "os": {
                    "description": "操作系统类型",
                    "type": "string",
                    "enum": [
                        "windows",
                        "linux"
                    ],
                    "example": "linux"
                },
                "platform": {
                    "description": "操作系统名称",
                    "type": "string",
                    "enum": [
                        "debian",
                        "centos",
                        "..."
                    ],
                    "example": "debian"
                },
                "type": {
                    "description": "agent类型",
                    "type": "string",
                    "example": "container-agent"
                },
                "version": {
                    "description": "agent版本号",
                    "type": "string",
                    "example": "1.0.0"
                }
            }
        },
        "api.Success": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "example": 0
                },
                "payload": {}
            }
        },
        "info.serverInfo": {
            "type": "object",
            "properties": {
                "agents": {
                    "description": "当前连接的agent数量",
                    "type": "integer",
                    "example": 10
                },
                "is_blocking": {
                    "description": "是否处于限流状态",
                    "type": "boolean",
                    "example": false
                },
                "version": {
                    "description": "服务端版本号",
                    "type": "string",
                    "example": "1.0.0"
                }
            }
        },
        "metrics.connection": {
            "type": "object"
        },
        "metrics.core": {
            "type": "object",
            "properties": {
                "core": {
                    "description": "所在物理核上的编号",
                    "type": "integer",
                    "example": 0
                },
                "cores": {
                    "description": "某块CPU上的编号",
                    "type": "integer",
                    "example": 0
                },
                "mhz": {
                    "description": "核心频率",
                    "type": "number",
                    "example": 1600
                },
                "model": {
                    "description": "CPU型号",
                    "type": "string",
                    "example": "Intel(R) Xeon(R) CPU E5-2620 v2 @ 2.10GHz"
                },
                "physical": {
                    "description": "物理CPU编号",
                    "type": "integer",
                    "example": 0
                },
                "processor": {
                    "description": "第几个核心",
                    "type": "integer",
                    "example": 0
                }
            }
        },
        "metrics.disk": {
            "type": "object",
            "properties": {
                "disks": {
                    "description": "逻辑分区",
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "example": [
                        "/boot",
                        "/home"
                    ]
                },
                "model": {
                    "description": "品牌型号",
                    "type": "string",
                    "example": "Samsung SSD 870 QVO 1TB"
                },
                "total": {
                    "description": "容量",
                    "type": "integer",
                    "example": 1000204886016
                },
                "type": {
                    "description": "磁盘类型",
                    "type": "string",
                    "enum": [
                        "HDD",
                        "SSD"
                    ],
                    "example": "HDD"
                }
            }
        },
        "metrics.dynamicInfo": {
            "type": "object",
            "properties": {
                "connections": {
                    "description": "连接列表",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/metrics.connection"
                    }
                },
                "process": {
                    "description": "进程列表",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/metrics.process"
                    }
                },
                "usage": {
                    "description": "usage数据",
                    "$ref": "#/definitions/metrics.usage"
                }
            }
        },
        "metrics.interfaceUsage": {
            "type": "object",
            "properties": {
                "bytes_recv": {
                    "description": "接收字节数",
                    "type": "integer",
                    "example": 24422
                },
                "bytes_sent": {
                    "description": "发送字节数",
                    "type": "integer",
                    "example": 6162729
                },
                "name": {
                    "description": "网卡名称",
                    "type": "string",
                    "example": "eth0"
                },
                "packets_recv": {
                    "description": "接收数据包数量",
                    "type": "integer",
                    "example": 4399
                },
                "packets_sent": {
                    "description": "发送数据包数量",
                    "type": "integer",
                    "example": 5699
                }
            }
        },
        "metrics.intf": {
            "type": "object",
            "properties": {
                "addrs": {
                    "description": "网卡上绑定的IP地址列表",
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "example": [
                        "192.168.1.100"
                    ]
                },
                "flags": {
                    "description": "网卡附加参数",
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "example": [
                        "UP",
                        "BROADCAST",
                        "RUNNING",
                        "MULTICAST"
                    ]
                },
                "index": {
                    "description": "网卡下标",
                    "type": "integer",
                    "example": 0
                },
                "mac": {
                    "description": "网卡mac地址",
                    "type": "string",
                    "example": "26:29:93:f2:84:22"
                },
                "mtu": {
                    "description": "网卡mtu",
                    "type": "integer",
                    "example": 1500
                },
                "name": {
                    "description": "网卡名称",
                    "type": "string",
                    "example": "lo"
                }
            }
        },
        "metrics.partition": {
            "type": "object",
            "properties": {
                "fstype": {
                    "description": "文件系统类型",
                    "type": "string",
                    "example": "NTFS"
                },
                "inodes": {
                    "description": "inode数量",
                    "type": "integer",
                    "example": 4072701
                },
                "mount": {
                    "description": "linux为挂载路径如/run，windows为盘符如C:",
                    "type": "string",
                    "example": "/boot"
                },
                "opts": {
                    "description": "附加信息",
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "example": [
                        "rw",
                        "nosuid",
                        "nodev"
                    ]
                },
                "total": {
                    "description": "总容量",
                    "type": "integer",
                    "example": 209666048
                }
            }
        },
        "metrics.partitionUsage": {
            "type": "object",
            "properties": {
                "free": {
                    "description": "剩余字节数",
                    "type": "integer",
                    "example": 232815064
                },
                "inode_free": {
                    "description": "inode剩余数量",
                    "type": "integer",
                    "example": 15998934
                },
                "inode_usage": {
                    "description": "inode使用率",
                    "type": "number",
                    "example": 5.64
                },
                "inode_used": {
                    "description": "inode已使用数量",
                    "type": "integer",
                    "example": 778282
                },
                "name": {
                    "description": "linux为挂载路径如/run，windows为盘符如C:",
                    "type": "string",
                    "example": "/"
                },
                "usage": {
                    "description": "磁盘使用率",
                    "type": "number",
                    "example": 6.27
                },
                "used": {
                    "description": "已使用字节数",
                    "type": "integer",
                    "example": 16920992
                }
            }
        },
        "metrics.process": {
            "type": "object",
            "properties": {
                "cmd": {
                    "description": "命令行",
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "example": [
                        "/usr/bin/zsh",
                        "-i"
                    ]
                },
                "conns": {
                    "description": "连接数",
                    "type": "integer",
                    "example": 16
                },
                "cpu_usage": {
                    "description": "CPU使用率",
                    "type": "number",
                    "example": 3.2
                },
                "id": {
                    "description": "进程ID",
                    "type": "integer",
                    "example": 5167
                },
                "listen": {
                    "description": "监听端口",
                    "type": "array",
                    "items": {
                        "type": "integer"
                    },
                    "example": [
                        8080,
                        9090
                    ]
                },
                "memory_usage": {
                    "description": "内存使用率",
                    "type": "number",
                    "example": 6.4
                },
                "parent_id": {
                    "description": "父进程ID",
                    "type": "integer",
                    "example": 1
                },
                "rss": {
                    "description": "物理内存数",
                    "type": "integer",
                    "example": 37654
                },
                "swap": {
                    "description": "swap内存数",
                    "type": "integer",
                    "example": 0
                },
                "user": {
                    "description": "用户",
                    "type": "string",
                    "example": "root"
                },
                "vms": {
                    "description": "虚拟内存数",
                    "type": "integer",
                    "example": 26754
                }
            }
        },
        "metrics.staticInfo": {
            "type": "object",
            "properties": {
                "cpu": {
                    "type": "object",
                    "properties": {
                        "cores": {
                            "description": "每个核心参数",
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/metrics.core"
                            }
                        },
                        "logical": {
                            "description": "逻辑核心数",
                            "type": "integer",
                            "example": 4
                        },
                        "physical": {
                            "description": "物理核心数",
                            "type": "integer",
                            "example": 2
                        }
                    }
                },
                "disks": {
                    "description": "物理磁盘列表",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/metrics.disk"
                    }
                },
                "gateway": {
                    "description": "网关地址",
                    "type": "string",
                    "example": "192.168.1.1"
                },
                "host": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "description": "主机名",
                            "type": "string",
                            "example": "DESKTOP-OQ8O3DR"
                        },
                        "uptime": {
                            "description": "启动时长",
                            "type": "integer",
                            "example": 1900
                        }
                    }
                },
                "interface": {
                    "description": "网卡列表",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/metrics.intf"
                    }
                },
                "kernel": {
                    "type": "object",
                    "properties": {
                        "arch": {
                            "description": "位数",
                            "type": "string",
                            "enum": [
                                "x86_64",
                                "i386",
                                "..."
                            ],
                            "example": "x86_64"
                        },
                        "version": {
                            "description": "内核版本",
                            "type": "string",
                            "example": "3.10.0-1062.el7.x86_64"
                        }
                    }
                },
                "memory": {
                    "type": "object",
                    "properties": {
                        "physical": {
                            "description": "物理内存大小",
                            "type": "integer",
                            "example": 33363566592
                        },
                        "swap": {
                            "description": "swap内存大小",
                            "type": "integer",
                            "example": 8589930496
                        }
                    }
                },
                "os": {
                    "type": "object",
                    "properties": {
                        "install": {
                            "description": "系统安装时间",
                            "type": "integer",
                            "example": 1661396019
                        },
                        "name": {
                            "description": "系统类型",
                            "type": "string",
                            "enum": [
                                "linux",
                                "windows"
                            ],
                            "example": "linux"
                        },
                        "platform_name": {
                            "description": "系统名称",
                            "type": "string",
                            "enum": [
                                "redhat",
                                "centos",
                                "debian",
                                "ubuntu",
                                "..."
                            ],
                            "example": "centos"
                        },
                        "platform_version": {
                            "description": "系统版本号",
                            "type": "string",
                            "example": "7.7.1908"
                        },
                        "startup": {
                            "description": "系统启动时间",
                            "type": "integer",
                            "example": 1661396019
                        }
                    }
                },
                "partitions": {
                    "description": "逻辑分区列表",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/metrics.partition"
                    }
                },
                "time": {
                    "description": "客户端时间",
                    "type": "integer",
                    "example": 1661396019
                },
                "user": {
                    "description": "用户列表",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/metrics.user"
                    }
                }
            }
        },
        "metrics.usage": {
            "type": "object",
            "properties": {
                "cpu": {
                    "type": "object",
                    "properties": {
                        "usage": {
                            "description": "CPU使用率(百分比)",
                            "type": "number",
                            "example": 2.3
                        }
                    }
                },
                "interface": {
                    "description": "网卡",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/metrics.interfaceUsage"
                    }
                },
                "memory": {
                    "type": "object",
                    "properties": {
                        "available": {
                            "description": "可用字节数",
                            "type": "integer",
                            "example": 201332
                        },
                        "free": {
                            "description": "剩余字节数",
                            "type": "integer",
                            "example": 94896
                        },
